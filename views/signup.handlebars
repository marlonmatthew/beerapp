<div class="screen"></div>
<div class="container">
  <div class="row welcome">
    <div class="col">
      <h2 id="signUpHeader">Sign Up Form</h2>
      <form class="signup">
        <div class="form-group">
          <label for="exampleInputEmail1">Email address</label>
          <input type="email" class="form-control" id="email-input" placeholder="Email">
        </div>
        <div class="form-group" id="nameField">
          <label for="InputfirstName">Name</label>
          <input type="firstName" class="form-control" id="name-input" placeholder="Name">
        </div>
        <div class="form-group" id="passwordField">
          <label for="exampleInputPassword1">Password</label>
          <input type="password" class="form-control" id="password-input" placeholder="Password">
        </div>
        <div style="display: none" id="alert" class="alert alert-danger" role="alert">
          <span class="glyphicon glyphicon-exclamation-sign" aria-hidden="true"></span>
          <span class="sr-only">Error:</span> <span class="msg"></span>
        </div>
        <button type="submit" class="btn btn-dark" id="signUpButton">Sign Up</button>
      </form>
      <p id="loginText2">Or log in <a href="login" id="signUplink">here</a></p>
    </div>
  </div>
</div>
